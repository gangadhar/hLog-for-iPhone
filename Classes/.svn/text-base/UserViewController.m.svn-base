	[self.view bringSubviewToFront:DatePicker];
	[super viewDidLoad];
}

-(NSMutableArray *)FillArray:(int)i
{   
	NSMutableArray *TypeArray;
	TypeArray=[[[NSMutableArray alloc]init]autorelease];	
	
	//////////// Routine
	if(i==7)
	{
		for(int i=0;i<=24;i++)
		{
			[TypeArray addObject:[NSString stringWithFormat:@"%d",i]];
		}
